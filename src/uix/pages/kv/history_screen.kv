#:import StoryCard uix.components.story_card
#:import StoryCard uix.components.page
<HistoryScreen>
    MDRelativeLayout:
        md_bg_color: 1,1,1,1
        Page:
            text: "HISTORY"
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            size_hint_y: 0.9
            size_hint_x: None
            width: self.parent.width * 0.8
            avatar: "../assets/images/darts.png"
            story: "../assets/images/story.png"
            callback: root.to_home
            MDBoxLayout:
                orientation: "vertical"
                padding:dp(2)
                id: container
                table_header: ''



