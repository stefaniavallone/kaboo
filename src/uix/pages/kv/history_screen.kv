<HistoryScreen>
    MDBoxLayout:
        orientation: "vertical"
        MDBoxLayout:
            size_hint_y:.10
            padding:dp(10)
            #md_bg_color: 255/255, 25/255, 25/255,  1
            orientation: "horizontal"
            MDIconButton:
                icon: "chevron-left"
                md_bg_color: app.theme_cls.primary_color
                on_release:
                    root.manager.transition.direction = 'right'
                    root.manager.current = 'home'
        MDBoxLayout:
            orientation: "horizontal"
            id: container
            #background_color_header: 255/255, 25/255, 25/255,  1
            #background_color_header: 255/255, 25/255, 25/255,  1
            #size_hint: 0.9, 0.6
            use_pagination: True

