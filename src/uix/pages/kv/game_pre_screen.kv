#:import MD3Card uix.components.md3card
<GamePreScreen>
    MDBoxLayout:
        adaptive_size: True
        pos_hint: {"center_x": .5, "center_y": .5}
        MD3Card:
            padding: 16
            text: "Start"
            md_bg_color: app.theme_cls.primary_color
            radius: dp(25)
            size_hint: None, None
            on_release: root.to_game()
