#:import StoryCard uix.components.story_card
#:import StoryCard uix.components.page
<TrophiesScreen>
    MDRelativeLayout:
        md_bg_color: 1,1,1,1
        Page:
            text: "TROPHIES"
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            size_hint_y: 0.9
            size_hint_x: None
            width: self.parent.width * 0.8
            avatar: "../assets/images/darts.png"
            story: "../assets/images/story.png"
            callback: root.to_home
            MDBoxLayout:
                orientation: "vertical"
                size_hint_x: 1
                pos_hint: {}
                padding:[dp(0),dp(30),dp(0),dp(0)]
                ScrollView:
                    MDList:
                        id: trophies_list