#:import OnboardingItem uix.components.onboarding_item

<GameSettingsRoundTimeScreen>

    OnboardingItem:
        source: "assets/images/game_settings/time.png"
        title: app.i18n._("GAMESETTINGS_ROUND_TIME_TITLE") # "Round time"
